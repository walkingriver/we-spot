{
  "name": "we-spot",
  "version": "0.6.18",
  "author": "Michael D. Callaghan",
  "homepage": "https://walkingriver.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "icons": "pwa-asset-generator src/assets/symbols/eyes-155625.svg ./src/assets/icon -e -c -f -t png -m src/manifest.webmanifest -i src/index.html -o false -b 'transparent'",
    "splash": "pwa-asset-generator src/assets/symbols/eyes-155625.svg ./src/assets/splash -h -f -t png -m src/manifest.webmanifest -i src/index.html -o false -b 'transparent'",
    "resources": "npm run icons && npm run splash"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^13.2.1",
    "@angular/common": "^14.0.2",
    "@angular/core": "^14.0.2",
